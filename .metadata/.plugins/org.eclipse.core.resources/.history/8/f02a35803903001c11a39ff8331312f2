#include "Stacking.h"
#include <stdint.h>

uint32_t arr_u32Temp[10u];

void copyStackingToRam( void ){
	__asm volatile(
			"IMPORT readStacking"
			"PRESERVE8 {TRUE}\n"
			"MRS R0,MSP \n"
			"PUSH {LR}"
	);
}


static readStacking( uint32_t *u32pAddress){
	for( uint8_t i=0;i<8u; i++){
		arr_u32Temp[i]= *u32pAddress;
		u32pAddress++;
	}
}







