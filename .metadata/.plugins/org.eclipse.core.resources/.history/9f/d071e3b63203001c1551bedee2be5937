//__asm volatile void copyStackingToRam( void ){
//	MRS R0, CONTROL
//}

__asm void SysTick_Handlers(void)
{
	IMPORT ReadStackFarme

	PRESERVE8 {TRUE}
    MRS R0, MSP
	PUSH {LR}
	BL ReadStackFarme
	POP {LR}
	BX LR
}







